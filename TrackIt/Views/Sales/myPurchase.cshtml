@{
}
@model List<StockClass>

<table class="table">
    <thead>
        <tr>
            <th>
                Product Name
            </th>
            <th>
                Serial Number
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var data in Model)
        {
            <tr>
                <td>
                    @data.Product.Name
                </td>
                <Td>
                    @data.serial_number
                </Td>
            </tr>
        }
    </tbody>
</table>